{
  "$schema": "https://schema.zeabur.app/template.json",
  "build": {
    "type": "dockerfile"
  },
  "start": {
    "command": "streamlit run app.py --server.port=${PORT:-8080} --server.address=0.0.0.0 --server.headless=true --server.enableCORS=false --server.enableXsrfProtection=false --browser.gatherUsageStats=false"
  },
  "env": {
    "PORT": {
      "default": "8080"
    }
  }
}
